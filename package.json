{
  "name": "react-context-switch",
  "version": "0.0.3",
  "description": "Simple conditional react  render switch using context",
  "repository": "https://github.com/VldMrgnn/react-switch",
  "main": "index.tsx",
  "scripts": {
    "test": "tsc --noEmit && ava",
    "format": "prettier --write **/*.tsx",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:beta": "npm publish --tag=next",
    "publish": "npm run clean && npm run build && npm publish"

  },
  "exports":{
    ".": {
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "react",
    "render",
    "switch",
    "conditional"
  ],
  "author": "Vlad Marginean",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "react-dom": ">=16.8"
  },
  "lint-staged": {
    "*.tsx": "prettier --write"
  }
}
